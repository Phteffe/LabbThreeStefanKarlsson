<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.iths.labbthreestefankarlsson.LabbThreeController">
   <Canvas fx:id="canvas" height="332.0" layoutX="1.0" layoutY="68.0" onMouseClicked="#canvasClicked" width="600.0" />
   <Spinner fx:id="spinner" editable="true" layoutX="53.0" layoutY="2.0" prefHeight="33.0" prefWidth="126.0" />
   <ColorPicker fx:id="colorPicker" layoutX="53.0" layoutY="35.0" prefHeight="33.0" prefWidth="126.0" />
   <Button fx:id="rectangleButton" layoutX="1.0" layoutY="2.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="52.0" text="Rectangle" />
   <Button fx:id="circleButton" layoutX="1.0" layoutY="35.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="52.0" text="Circle" />
   <CheckBox fx:id="checkBox" layoutX="500.0" layoutY="6.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="97.0" text="Select Mode" />
   <Button fx:id="deleteButton" layoutX="179.0" layoutY="2.0" mnemonicParsing="false" onMouseClicked="#deleteMarkedShapes" prefHeight="33.0" prefWidth="52.0" text="Delete" />
   <Button fx:id="undoButton" layoutX="179.0" layoutY="35.0" mnemonicParsing="false" onMouseClicked="#undo" prefHeight="33.0" prefWidth="52.0" text="Undo" />
   <Button fx:id="changeColorButton" layoutX="231.0" layoutY="2.0" mnemonicParsing="false" onMouseClicked="#changeColorOfSelected" prefHeight="33.0" prefWidth="97.0" text="Change Color" />
   <Button fx:id="changeSizeButton" layoutX="231.0" layoutY="35.0" mnemonicParsing="false" onMouseClicked="#changeSizeOfSelected" prefHeight="33.0" prefWidth="97.0" text="Change Size" />
   <Button fx:id="saveButton" onMouseClicked="#saveFile" layoutX="516.0" layoutY="35.0" mnemonicParsing="false" text="Save" />
</AnchorPane>
